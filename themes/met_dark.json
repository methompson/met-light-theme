{
	"$schema": "vscode://schemas/color-theme",
	"name": "Mat's Dark Theme",
	"include": "./solarized-dark-color-theme.json",
	"tokenColors": [
		{
			"name": "Go Text",
			"scope": [
				"source.go",
			],
			"settings": {
				"foreground": "#d7dda3"
			}
		},
		{
			"name": "Class",
			"scope": [
				"entity.name.type.class",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#f6e0fd"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"variable.language.dart"
			],
			"settings": {
				"foreground": "#00d9ff"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.other.property",
				"variable.other.readwrite",
				"variable.other",
				"storage.type.annotation.dart",
				"storage.type.string.go",
				"storage.type.boolean.go",
				"storage.type.numeric.go",
				"storage.type.error.go"
			],
			"settings": {
				"foreground": "#00ffb3"
			}
		},
		{
			"name": "Dot Values",
			"scope": [
				"entity.name.function.member",
				"meta.embedded"
			],
			"settings": {
				"foreground": "#b8ff47"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#3be2ff"
			}
		},
		{
			"name": "JSON property",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#3be2ff"
			}
		},
		{
			"name": "Enums",
			"scope": [
				"entity.name.type.enum",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#c1ff5d"
			}
		},
		{
			"name": "Keyword Controls",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#ff9868"
			}
		},
		{
			"name": "Boolean",
			"scope": [
				"constant.language.dart",
				"constant.language.json",
				"constant.language",
			],
			"settings": {
				"foreground": "#ffcc5d"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#a8ff61"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#ddadad"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#ff7474"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function.go",
				"support.function.builtin.go"
			],
			"settings": {
				"foreground": "#ff8fd4"
			}
		},
	],
	"colors": {
		"editor.background": "#002e3f",
		"sideBar.background": "#002836",
		"panel.border": "#00506d",

		"editor.selectionBackground": "#ff000079",
		"editor.selectionHighlightBackground": "#91ff0042"
	}
}